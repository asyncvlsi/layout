LAYER LEGALIZER
   SAME_DIFF_SPACING 0.900000 ;
   ANY_DIFF_SPACING 3.600000 ;
   WELLTAP_ADJUST 4.800000 ;
END LEGALIZER

LAYER pwell
    MINWIDTH 3.000000 ;
    SPACING 1.800000 ;
    OPPOSPACING 0.000000 ;
    MAXPLUGDIST 150.000000 ;
    OVERHANG 1.800000 ;
END pwell

LAYER nwell
    MINWIDTH 3.000000 ;
    SPACING 1.800000 ;
    OPPOSPACING 0.000000 ;
    MAXPLUGDIST 150.000000 ;
    OVERHANG 1.800000 ;
END nwell

MACRO mycell
    VERSION mycell
        UNPLUG
        LAYER pwell ;
        RECT 0.600000 1.500000 15.600000 12.000000 ;
        END
        LAYER nwell ;
        RECT 0.600000 12.000000 15.600000 21.900000 ;
        END
    END VERSION
END mycell

MACRO welltap_svt
   VERSION welltap_svt
   PLUG
   LAYER pwell ;
   RECT 1.500000 1.500000 4.200000 7.800000
   END
   LAYER nwell ;
   RECT 1.500000 7.800000 4.200000 8.400000
   END
   END VERSION
END welltap_svt
